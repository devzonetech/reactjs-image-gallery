{
  "name": "reactjs-image-gallery",
  "version": "1.1.7",
  "description": "Responsive, flexible and easy customizable react images gallery library. List images in table view order, thumbnails, onclick show big image preview.",
  "keywords": [
    "react images list",
    "images gallery",
    "images list",
    "photos list",
    "responsive images gallery",
    "image gallery",
    "photo gallery",
    "pictures gallery",
    "pix gallery",
    "react gallery",
    "image gallery",
    "thumbnail images",
    "gallery",
    "slider",
    "component",
    "photo"
  ],
  "author": "Devzone Tech Ltd.",
  "license": "MIT",
  "repository": "devzonetech/reactjs-image-gallery",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rm -rf ./dist && rollup -c && minify ./dist/index.js --mangle.keepClassName --deadcode.keepFnArgs --outFile ./dist/index.js",
    "start": "rollup -c -w",
    "prepare": "npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "prop-types": ">= 15.x.x",
    "react": ">= 16.x.x",
    "react-dom": ">= 16.x.x"
  },
  "devDependencies": {
    "@svgr/rollup": "^2.4.1",
    "ajv": "^6.11.0",
    "babel-core": "^6.26.3",
    "babel-minify": "^0.5.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.2.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-standard": "^3.1.0",
    "gh-pages": "^1.2.0",
    "node-sass": "^4.13.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.3.0",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-url": "^1.4.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {}
}